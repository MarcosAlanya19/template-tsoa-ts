{
  "name": "restserver",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"yarn optimize-nodemon\" \"yarn optimize-routes\"",
    "dev:complete": "concurrently \"yarn optimize-nodemon\" \"yarn optimize-spec-routes\"",
    "dev:init": "tsoa spec-and-routes",
    "clean": "rmdir /s dist",
    "build": "tsoa spec-and-routes && tsc && tsc-alias",
    "build:routes": "tsoa routes",
    "build:spec": "tsoa spec",
    "build:tsoa": "tsoa spec-and-routes",
    "start": "node dist/app.js",
    "file": "node -r tsconfig-paths/register -r ts-node/register",
    "optimize-nodemon": "nodemon",
    "optimize-routes": "nodemon -x tsoa routes",
    "optimize-spec-routes": "nodemon -x tsoa spec-and-routes"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "bcrypt": "^5.1.0",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fs-extra": "^11.1.1",
    "jsonwebtoken": "^9.0.1",
    "mongoose": "^7.3.0",
    "mongoose-paginate-v2": "^1.7.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  }
}
